            {
              "$type": "Models.Manager, Models",
              "Code": "using APSIM.Shared.Utilities;\r\nusing Models.GrazPlan;\r\nusing Models.Utilities;\r\nusing Models.Soils;\r\nusing Models.PMF;\r\nusing Models.Core;\r\nusing System;\r\nusing System.Linq;\r\n\r\nnamespace Models\r\n{\r\n    [Description(\r\n    \"<b>Supplement store management.</b>\" +\r\n    \"<li> When animals are in feedlot, they are fed a set amount each day.</li>\" +\r\n    \"<li> Will restock the supplement store when the store reaches a minimum level.</li>\"\r\n    )]\r\n    [Serializable]\r\n    public class ManageSupplement : Model\r\n    {\r\n        [Link] Clock clock;\r\n        [Link] Stock animals;\r\n        [Link] Supplement supplement;\r\n\r\n        [Description(\"Name of zone representing the feedlot\")]\r\n        [Display(Type=DisplayType.Model, ModelType=typeof(Zone))]\r\n        public IModel FeedlotZone { get; set; }\r\n\r\n        [Description(\"The amount of supplement to feed each animal each day (kg DM /head /day)\")]\r\n        public double SupplementAmount { get; set; }\r\n\r\n        [Description(\"The type of supplement to feed the animals\")]\r\n        public string SupplementType { get; set; }\r\n\r\n        [Description(\"Amount of supplement remaining before the store is replenished - i.e. to trigger a buy supplement event (kg DM)\")]\r\n        public double SupplementMinAmount { get; set; }\r\n\r\n        [Description(\"Maximum supplement store - top up the store to this value when buying (kg DM)\")]\r\n        public double SupplementMaxAmount { get; set; }\r\n\r\n        // ============================================================\r\n        // Called every day to perform management actions\r\n        // ============================================================\r\n        [EventSubscribe(\"DoManagement\")]\r\n        private void OnDoManagement(object sender, EventArgs e)\r\n        {\r\n            // Determine how many animals are in feedlot.\r\n            var animalGroupsInFeedlot = animals.AnimalGroups.Where(group => group.PaddOccupied.Name == FeedlotZone.Name);\r\n            var totalNumAnimalsInFeedlot = animalGroupsInFeedlot.Sum(group => group.NoAnimals);\r\n\r\n            // If there are animals in feedlot then feed them.\r\n            if (totalNumAnimalsInFeedlot > 0)\r\n                supplement.Feed(SupplementType, SupplementAmount * totalNumAnimalsInFeedlot, FeedlotZone.Name);     \r\n\r\n            // Restock the supplement store if amount falls below SupplementMinAmount kg DM\r\n            if (supplement.Stores[1].Stored <= SupplementMinAmount)       \r\n                supplement.Buy(SupplementMaxAmount - supplement.Stores[1].Stored, SupplementType);\r\n        }\r\n    }\r\n}\r\n",
