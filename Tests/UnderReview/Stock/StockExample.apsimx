            {
              "$type": "Models.Manager, Models",
              "Code": "/*-------------------------------------------------------------------------------------\r\nSheep grazing wheat when conditions are suitable, and in feedlot the rest of the time.\r\n---------------------------------------------------------------------------------------\r\n\r\nIn this script animals are bought and sold within a specified date window.\r\nThey are fed supplement in a feedlot at a set rate, but graze a wheat crop when a criteria is met.\r\nAnimals are moved from the wheat crop and back to feedlot when a second criteria is met.\r\n\r\nBased on Sprague SJ, Kirkegaard JA, Dove H, Graham JM, McDonald SE, Kelman WM (2015) Integrating dual-purpose wheat and canola into high-rainfall livestock systems in south-eastern Australia. 1. Crop forage and grain yield. Crop and Pasture Science 66(4), 365-376. \r\n\r\nActivities in this manager:\r\n1. Buy animals at start of year & put in feedlot\r\n2. Move animals from feedlot to crop when ready to graze\r\n3. Move animals from crop to feedlot \r\n4. Shear all animals on specified date\r\n5. Sell all animals at end of year\r\n\r\nNOTES\r\n1. When the animals are in the feedlot and an animal dies during the day, the supplement has already been fed into the feedlot\r\n   based on the number of animals in the feedlot at the start of the day. This means the remaining animals have access to slightly\r\n   more supplement and causes a spike in supp intake graph\r\n2. When sheep are culled for age + purchased to maintain stocking rate, several groups of sheep are created.\r\n   This causes irregular amounts of supplement to be fed.\r\n--------------------------------------------------------------------------------------*/\r\n\r\n\r\nusing System.Linq;\r\nusing System;\r\nusing System.Collections.Generic;\r\nusing Models.Core;\r\nusing Models.PMF;\r\nusing Models.Functions;\r\nusing Models.Soils;\r\nusing Models.Utilities;\r\nusing Models.GrazPlan;\r\nusing APSIM.Shared.Utilities;\r\nusing System.Diagnostics;\r\n\r\nnamespace Models\r\n{\r\n    [Serializable]\r\n    [Description(\r\n        \"<p><b>Animals grazing a crop when conditions are suitable, and in feedlot the rest of the time.</b></p>\" +\r\n        \"In this script animals are bought and sold within a specified date window. \" +\r\n        \"They are fed supplement in a feedlot at a set rate, but graze a wheat crop when a criteria is met. \" +\r\n        \"Animals are moved from the wheat crop and back to feedlot when a second criteria is met.\")]    \r\n    public class Script : Model\r\n    {\r\n        [Link] Clock clock;\r\n        [Link] Stock Animals;\r\n        [Link] Supplement Feed;\r\n        [Link] Summary Summary;\r\n        [Link] IPlant plant;\r\n        [Link] Soil Soil;\r\n        [Link] Zone Paddock;\r\n        [Link] ScriptCompiler compiler;\r\n\r\n        private CSharpExpressionFunction expressionMoveAnimalsToField;\r\n        private CSharpExpressionFunction expressionMoveAnimalsToFeedlot;\r\n\r\n        public enum SellPolicyType\r\n        {\r\n            Date,\r\n            PercentageHerdWhenAverageWeightGreaterThan,\r\n            PercentageHerdWhenAverageAgeGreaterThan,\r\n            CulledForAge,\r\n            CulledForWeight,\r\n            SellYoung\r\n        }\r\n\r\n        ////////////////////////////////////////////////////////\r\n        [Separator(\"Buying of animals\")]\r\n\r\n        [Description(\"Buy animals?\")] \r\n        public bool Buying { get; set; }\r\n\r\n        [Description(\"Stock: genotype (e.g. Small Merino, Angus, Friesian):\")] \r\n        [Display(Values = \"GetGenotypes\")]\r\n        public string Genotype { get; set; }\r\n\r\n        [Description(\"Stock: sex (Female, Male, Castrate):\")] \r\n        public ReproductiveType Sex { get; set; }\r\n\r\n        [Description(\"Date to buy animals (dd-mmm)\")] \r\n        [Display(EnabledCallback = \"BuyingEnabled\")]\r\n        public string BuyDate { get; set; }\r\n\r\n        [Description(\"Age of bought animals (months): \")] \r\n        [Display(EnabledCallback = \"BuyingEnabled\")]\r\n        public double BuyAge { get; set; }\r\n\r\n        [Description(\"Weight of bought animals (kg): \")] \r\n        [Display(EnabledCallback = \"BuyingEnabled\")]\r\n        public double BuyWeight { get; set; }\r\n\r\n        [Description(\"Zero denotes not lactating; 1 or more denotes the time (days) since parturition:\")] \r\n        [Display(EnabledCallback = \"BuyingEnabled\")]\r\n        public int BuyLactating { get; set; }\r\n\r\n        [Description(\"Number of animals to buy: \")] \r\n        [Display(EnabledCallback = \"BuyingEnabled\")]\r\n        public int NumAntoBuy { get; set; }\r\n\r\n        ////////////////////////////////////////////////////////\r\n        [Separator(\"Selling of animals\")]\r\n\r\n        [Description(\"Selling animals?\")] \r\n        public bool Selling { get; set; }\r\n\r\n        [Description(\"First date to consider selling animals (dd-mmm)\")] \r\n        public string SaleFirstDate { get; set; }\r\n\r\n        [Description(\"Last date to consider selling animals (dd-mmm)\")] \r\n        public string SaleLastDate { get; set; }\r\n\r\n        [Description(\"Selling policy \")] \r\n        [Display(EnabledCallback = \"SellingEnabled\")]\r\n        public SellPolicyType SellPolicy { get; set; }\r\n\r\n        [Description(\"Age at which to sell (months): \")] \r\n        [Display(EnabledCallback = \"SellingAgeEnabled\")]\r\n        public int SaleAge { get; set; }\r\n\r\n        [Description(\"Weight at which to sell (kg): \")] \r\n        [Display(EnabledCallback = \"SellingWeightEnabled\")]\r\n        public double SaleWeight { get; set; }\r\n\r\n        [Description(\"Percentage of animals to sell (%)\")] \r\n        [Display(EnabledCallback = \"SellingEnabled\")]\r\n        public double SalePercentAnimals { get; set; }\r\n\r\n        ////////////////////////////////////////////////////////\r\n        [Separator(\"Rule for moving animals into and out of paddock\")]\r\n\r\n        [Description(\"Name of paddock to move animals in to:\")] \r\n        [Display(Type = DisplayType.Model, ModelType = typeof(Zone))]\r\n        public IModel PaddockToMoveAnimalsTo { get; set; }\r\n\r\n        [Description(\"First date to consider moving animals:\")] \r\n        public string GrazingWindowStartDate { get; set; }\r\n\r\n        [Description(\"Last date to consider moving animals:\")] \r\n        public string GrazingWindowEndDate { get; set; }\r\n\r\n        [Description(\"Criteria to move animals to field\")]\r\n        public string ExpressionToMoveAnimalsToField { get; set; }\r\n\r\n        [Description(\"Criteria to move animals to feedlot\")]\r\n        public string ExpressionToMoveAnimalsToFeedlot { get; set; }\r\n\r\n\r\n        ////////////////////////////////////////////////////////\r\n        [Separator(\"Shearing\")]\r\n        [Description(\"Date to shear animals (dd-mmm)\")] \r\n        public string ShearDate { get; set; }\r\n\r\n        [Description(\" \")] \r\n        public string Dummy { get; set; }\r\n\r\n        ////////////////////////////////////////////////////////////////////////////\r\n        // Callback properties to control disabling of properties.\r\n        ////////////////////////////////////////////////////////////////////////////\r\n        public bool BuyingEnabled { get { return Buying; } }\r\n\r\n        public bool SellingEnabled { get { return Selling; } }\r\n\r\n        public bool SellingAgeEnabled\r\n        { \r\n            get\r\n            { \r\n                return Selling &&\r\n                (SellPolicy == SellPolicyType.PercentageHerdWhenAverageAgeGreaterThan || SellPolicy == SellPolicyType.CulledForAge);\r\n            } \r\n        }\r\n\r\n        public bool SellingWeightEnabled\r\n        { \r\n            get\r\n            { \r\n                return Selling &&\r\n                (SellPolicy == SellPolicyType.PercentageHerdWhenAverageWeightGreaterThan || SellPolicy == SellPolicyType.CulledForWeight);\r\n            } \r\n        }\r\n\r\n        ////////////////////////////////////////////////////////////////////////////\r\n        // Outputs\r\n        ////////////////////////////////////////////////////////////////////////////\r\n        // <summary>Number of animals sold.</summary>\r\n        public int NumberSold { get; set; }\r\n\r\n        // <summary>Description of something that has happened.</summary>\r\n        public string EventName { get; set; }\r\n\r\n        // <summary>Return a list of genotypes to the properties tab.</summary>\r\n        public string[] GetGenotypes()\r\n        {\r\n            if (Animals != null)\r\n                return Animals.Genotypes.Names.ToArray();\r\n            return null;\r\n        }\r\n\r\n        public string CurrentLocation { get; set; }  // location of the sheep\r\n\r\n\r\n\r\n        // ============================================================\r\n        // Called once at beginning of simulation run\r\n        // ============================================================\r\n        [EventSubscribe(\"StartOfSimulation\")]\r\n        private void OnStartOfSimulation(object sender, EventArgs e)\r\n        {\r\n            if (string.IsNullOrEmpty(ExpressionToMoveAnimalsToField))\r\n                throw new Exception(\"You must specify a criteria for moving animals to the field\");\r\n            expressionMoveAnimalsToField = new CSharpExpressionFunction();\r\n            expressionMoveAnimalsToField.Parent = this;\r\n            expressionMoveAnimalsToField.Expression = \"Convert.ToDouble(\" + ExpressionToMoveAnimalsToField + \")\";\r\n            expressionMoveAnimalsToField.SetCompiler(compiler);\r\n            expressionMoveAnimalsToField.CompileExpression();\r\n\r\n            if (string.IsNullOrEmpty(ExpressionToMoveAnimalsToFeedlot))\r\n                throw new Exception(\"You must specify a criteria for moving animals to the feedlot\");\r\n            expressionMoveAnimalsToFeedlot = new CSharpExpressionFunction();\r\n            expressionMoveAnimalsToFeedlot.Parent = this;\r\n            expressionMoveAnimalsToFeedlot.Expression = \"Convert.ToDouble(\" + ExpressionToMoveAnimalsToFeedlot + \")\";\r\n            expressionMoveAnimalsToFeedlot.SetCompiler(compiler);\r\n            expressionMoveAnimalsToFeedlot.CompileExpression();\r\n\r\n            CurrentLocation = \"\";\r\n        }\r\n\r\n\r\n\r\n        // ============================================================\r\n        // Management that happens every day.\r\n        // ============================================================\r\n        [EventSubscribe(\"DoManagement\")]\r\n        private void OnDoManagement(object sender, EventArgs e)\r\n        {\r\n            EventName = string.Empty;\r\n\r\n            // Buy animals\r\n            if (Buying && DateUtilities.DatesAreEqual(BuyDate, clock.Today))\r\n            {\r\n                var purchase = new StockBuy();\r\n                purchase.Genotype = Genotype;   // \"small merino\"\r\n                purchase.Number = NumAntoBuy;\r\n                purchase.Sex = Sex;               // \"wethers\"\r\n                purchase.Age = BuyAge;\r\n                purchase.Weight = BuyWeight;\r\n                purchase.Lactating = BuyLactating;\r\n                Animals.Buy(purchase);\r\n                Animals.Move(\"Feedlot\");\r\n                EventName = NumAntoBuy + \" animals bought\";\r\n\r\n                CurrentLocation = \"Feedlot\";\r\n            }\r\n\r\n            // Sell animals\r\n            if (SellingEnabled && DateUtilities.WithinDates(SaleFirstDate, clock.Today, SaleLastDate))\r\n            { \r\n                if (SellPolicy == SellPolicyType.PercentageHerdWhenAverageWeightGreaterThan || \r\n                    SellPolicy == SellPolicyType.PercentageHerdWhenAverageAgeGreaterThan)\r\n                {\r\n                    bool doSell = false;\r\n                    if (SellPolicy == SellPolicyType.Date)\r\n                        doSell = true;\r\n                    else if (SellPolicy == SellPolicyType.PercentageHerdWhenAverageWeightGreaterThan && Animals.WeightAll >= SaleWeight)\r\n                        doSell = true;\r\n                    else if (SellPolicy == SellPolicyType.PercentageHerdWhenAverageAgeGreaterThan && Animals.AgeMonthsAll >= SaleAge)\r\n                        doSell = true;\r\n\r\n                    if (doSell)\r\n                    {\r\n                        NumberSold = Convert.ToInt32(Animals.NumberAll * SalePercentAnimals / 100.0);\r\n                        Animals.Sell(NumberSold);\r\n                        EventName = NumberSold + \" animals sold\";\r\n                    }\r\n                }\r\n                else\r\n                {\r\n                    IEnumerable<AnimalGroup> animalsToSell = null;\r\n                    if (SellPolicy == SellPolicyType.CulledForAge)\r\n                    {\r\n                        // split older animals into a new group\r\n                        animalsToSell = Animals.SplitByAge(Convert.ToInt32(SaleAge * 30)); // months to days\r\n                    }\r\n                    else if (SellPolicy == SellPolicyType.CulledForWeight)\r\n                    {\r\n                        // split heavier animals into a new group\r\n                        animalsToSell = Animals.SplitByWeight(SaleWeight);\r\n                    }\r\n                    else if (SellPolicy == SellPolicyType.SellYoung)\r\n                    {\r\n                        // split young (offspring) into new groups\r\n                        animalsToSell = Animals.SplitByYoung();\r\n                    }\r\n                    if (animalsToSell != null)\r\n                    {\r\n                        var numberSellableAnimals = animalsToSell.Sum(g => g.NoAnimals);\r\n                        var numberAnimalsToSell = Convert.ToInt32(numberSellableAnimals * SalePercentAnimals / 100.0);\r\n                        if (numberAnimalsToSell > 0)\r\n                            Animals.Sell(numberAnimalsToSell, animalsToSell);\r\n                    }\r\n                }\r\n            }\r\n\r\n\r\n            // Determine whether the animals are in the feedlot.\r\n            bool animalsAreInFeedlot = Animals.AnimalGroups.FirstOrDefault(group => group.PaddOccupied.Name == \"Feedlot\") != null;\r\n            bool inGrazingWindow = DateUtilities.WithinDates(GrazingWindowStartDate, clock.Today, GrazingWindowEndDate);\r\n\r\n            if (animalsAreInFeedlot)\r\n            {\r\n                   // Animals are in feedlot. Determine if they can be moved to paddock.\r\n                if (inGrazingWindow && expressionMoveAnimalsToField.Value() == 1)\r\n                {\r\n                    Animals.Move(PaddockToMoveAnimalsTo.Name);\r\n                    EventName = \"Animals moved to paddock\";\r\n                    CurrentLocation = \"Paddock\";\r\n                }\r\n            }\r\n            else\r\n            {\r\n                // Animals are in paddock. Determine if they need to be moved to feedlot.\r\n                if (!inGrazingWindow || expressionMoveAnimalsToFeedlot.Value() == 1)\r\n                {\r\n                    Animals.Move(\"Feedlot\");\r\n                    CurrentLocation = \"Feedlot\";\r\n                    EventName = \"Animals moved to feedlot\";\r\n                }\r\n            }\r\n\r\n            // Shear all animals on specified date\r\n            if (DateUtilities.DatesEqual(ShearDate, clock.Today))\r\n            {\r\n                Animals.Shear(shearAdults: true, shearYoung: true);\r\n                EventName = \"Animals shorn\";\r\n            }\r\n        }\r\n    }\r\n}\r\n",
              "Parameters": [
