    <Compile Remove="Report\RowEnumerator.cs" />
    <Compile Remove="Resources\Scripts\CSharpExpressionTemplate.cs" />
    <Compile Remove="Resources\Scripts\GraphPanelScriptTemplate.cs" />
    <Compile Remove="Storage\FBDataStore.cs" />
